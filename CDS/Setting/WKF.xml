<?xml version="1.0" encoding="utf-8"?>
<!--
使用說明:
<WKF>
  <service>
    <ipSecurity>
      <add start="192.168.3.1" end="192.168.3.255"/> 範圍IP 「*」格式不支援
      <add start="192.168.1.1" end="192.168.1.1"/> 單一IP 起始輸入一樣IP
    </ipSecurity>
  </service>
</WKF>
-->

<WKF>
  <service>
    <ipSecurity>
       <add start="10.201.5.2" end="10.201.5.255"/>
       <add start="172.20.20.2" end="172.20.20.255"/>
       <add start="10.190.24.2" end="10.190.24.255"/>
    </ipSecurity>
  </service>
</WKF>